{
  "kind": "collectionType",
  "collectionName": "blog_post_tests",
  "info": {
    "singularName": "blog-post-test",
    "pluralName": "blog-post-tests",
    "displayName": "BlogPostTest"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "blog.blog",
        "blog.paragraph",
        "blog.image"
      ]
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "blog_post_tests"
    }
  }
}
